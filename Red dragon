local assetId = 72891544399694 
local soundId = "rbxassetid://0" -- Sound ID (replace if needed)

-- Load the okarunslasheffect (Folder)
local okarunslasheffect = game:GetObjects("rbxassetid://" .. assetId)[1]

-- Create and play sound
local sound = Instance.new("Sound")
sound.SoundId = soundId
sound.Parent = game.Workspace
sound:Play()

-- Ensure the okarunslasheffect was loaded properly
if okarunslasheffect then
    local character = game.Players.LocalPlayer.Character
    if character then
        -- Wait for character parts to be loaded
        local shoulder = character:WaitForChild("Head")

        if shoulder then
            -- Parent the okarunslasheffect to the workspace
            okarunslasheffect.Parent = workspace

            -- Iterate through all parts in the folder and position them
            for _, part in pairs(okarunslasheffect:GetDescendants()) do
                if part:IsA("BasePart") then
                    -- Position the part relative to the right arm
                    part.Position = shoulder.Position + Vector3.new(0, 0, 0) -- Adjust offset if necessary
                    part.CanCollide = false -- Disable collision
                    part.Anchored = false    -- Anchor parts to keep them in place

                    -- Create a weld for each part
                    local weld = Instance.new("Weld")
                    weld.Part0 = shoulder -- The part you're welding to
                    weld.Part1 = part -- The part you're welding
                    weld.C0 = CFrame.new(0, 2, 0) * CFrame.Angles(0, 0, 0)
                    weld.Parent = part -- Parent the weld to the part being welded
                    
                   
                end
            end
        end
    end
end

